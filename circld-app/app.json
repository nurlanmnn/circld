{
  "expo": {
    "name": "circld-app",
    "slug": "circld-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_circld_notext.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_circld_notext.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_circld_notext.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo_circld_notext.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
